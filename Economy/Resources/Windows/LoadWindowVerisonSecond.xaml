<Window x:Class="Economy.Resources.Windows.LoadWindowVerisonSecond"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Economy.Resources.Windows"
        mc:Ignorable="d"
        Title="LoadWindowVerisonSecond"
        Height="150"
        Width="300">
    <Grid>
        <StackPanel Margin="10">
            <Label>Данные экспортируются...</Label>
            <Label>Пожалуйста подождите.</Label>
            <ProgressBar Margin="10"
                         Name="pbStatus"
                         Minimum="0"
                         Maximum="100"
                         Width="200"
                         Height="20"
                         VerticalAlignment="Center"
                         HorizontalAlignment="Center" />
        </StackPanel>
    </Grid>
</Window>